{
  "expo": {
    "name": "Bike Tour Assistant",
    "slug": "bike-tour-assistant",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#455a64"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bike-tour-assistant"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#455a64"
      },
      "permissions": [],
      "package": "com.bike_tour_assistant"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5d497896-ba21-4721-acfe-f98a022086c7"
      }
    },
    "owner": "aktyn",
    "plugins": [
      "react-native-ble-plx",
      "expo-font"
    ]
  }
}
